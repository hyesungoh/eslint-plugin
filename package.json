{
  "name": "@hyesungoh/eslint-plugin",
  "version": "0.0.3",
  "description": "eslint and prettier configuration",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyesungoh/eslint-plugin"
  },
  "license": "MIT",
  "type": "commonjs",
  "author": {
    "name": "hyesungoh",
    "email": "haesungoh414@gmail.com",
    "url": "https://www.hyesungoh.xyz"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "build": "rollup --config",
    "version": "changeset version && yarn install --no-immutable",
    "publish": "changeset publish"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.8.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "rollup": "^3.10.1"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "typescript": "^4.5.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  }
}